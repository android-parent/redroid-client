{
  "expo": {
    "name": "redroid-client",
    "slug": "redroid-client",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.redroid.ios"
    },
    "android": {
      "package": "io.redroid.android"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}